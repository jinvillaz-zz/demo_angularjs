    "style-loader": "~0.13.1",
    "url-loader": "~1.0.1",
    "webpack": "~2.2.1",
    "webpack-fail-plugin": "~1.0.5"
  },
  "eslintConfig": {
    "globals": {
      "expect": true,
      "angular": true,
      "Konva": true,
      "$": true
    },
    "root": true,
    "env": {
      "browser": true,
      "jasmine": true
    },
    "parser": "babel-eslint",
    "extends": [
      "xo-space/esnext"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "angular/no-service-method": 0,
      "max-params": 0,
      "no-console": [
        2,
        {
          "allow": [
            "info",
            "error"
          ]
        }
      ],
      "max-len": [
        2,
        100,
        2,
        {
          "ignoreComments": true,
          "ignoreUrls": true
        }
      ],
      "semi": [
        "error",
        "always",
        {
          "omitLastInOneLineBlock": true
        }
      ],
      "space-before-function-paren": [
        2,
        {
          "anonymous": "never",
          "named": "never"
        }
      ],
      "indent": [
        "error",
        4
      ],
      "newline-per-chained-call": [
        "error",
        {
          "ignoreChainWithDepth": 2
        }
      ],
      "valid-jsdoc": [
        "error",
        {
          "requireReturn": false
        }
      ],
      "arrow-parens": [
        "error",
        "always"
      ],
      "prefer-arrow-callback": "error",
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "linebreak-style": 0,
      "no-new": 0,
      "radix": 0,
      "max-depth": [
        "error",
        5
      ],
      "no-useless-escape": 0
    }
  }
}
